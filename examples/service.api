package bundles

facade Bundle<1> {

    data BundleChanges {
        ID              string
        Method          string
        Args            []Any
        Requires        []string
    }

    param BundleChangesParams {
        BundleDataYAML  string
        BundleURL       string
    }

    result BundleChangesResults {
        Changes         []BundleChanges
        Errors          []string
    }

    GetChanges(BundleChangesParams, Something) (BundleChangesResults, error)
}
